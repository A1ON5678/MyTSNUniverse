	<!-- When the vessel moves out of the sector on the left side, it will appear on the right side -->
	<event name_arme="PositionLeftX">
	 <if_object_property name="Artemis" property="positionX" comparator="GREATER" value="99990"/>
	 <set_object_property property="positionX" name="Artemis" value="100"/>
     <!-- Initiate the destroy pattern by setting the flag and recording the current sector position -->
	 <set_variable name="IsDestroy" value="1" />	 
	 <set_variable name="destroy_x" value="x" />
	 <set_variable name="destroy_z" value="z" />
     <!-- This records the next sector, once the destroy pattern finishes, the create pattern will start using the new coordinates. -->
	 <set_variable name="x" value="x-1" />

     <destroy_near type="all" radius="100000" centerX="50000" centerY="0.0" centerZ="50000" />
	 
	 <!-- Comms gets a message for current sector location -->
	 <incoming_comms_text from="DSTAN AND ONBOARD NAV">Current position (|x|, |z|)</incoming_comms_text>
	</event>
	
	<!-- When the vessel moves out of the sector on the bottom side, it will appear on the top side -->
	<event name_arme="PositionBottomZ">
	 <if_object_property name="Artemis" property="positionZ" comparator="GREATER" value="99990"/>
	 <set_object_property property="positionZ" name="Artemis" value="100"/>
	 <!-- Initiate the destroy pattern by setting the flag and recording the current sector position -->
	 <set_variable name="IsDestroy" value="1" />	 
	 <set_variable name="destroy_x" value="x" />
	 <set_variable name="destroy_z" value="z" />	
	 <!-- This records the next sector, once the destroy pattern finishes, the create pattern will start using the new coordinates. -->		 
	 <set_variable name="z" value="z-1" />

     <destroy_near type="all" radius="100000" centerX="50000" centerY="0.0" centerZ="50000" />
	 
	 <!-- Comms gets a message for current sector location -->
	 <incoming_comms_text from="DSTAN AND ONBOARD NAV">Current position (|x|, |z|)</incoming_comms_text>
	</event>
	
	<!-- When the vessel moves out of the sector on the right side, it will appear on the left side -->
	<event name_arme="PositionRightX">
	 <if_object_property name="Artemis" property="positionX" comparator="LESS" value="10"/>
	 <set_object_property property="positionX" name="Artemis" value="99900"/>
	 <set_variable name="IsDestroy" value="1" />	 
	 <set_variable name="destroy_x" value="x" />
	 <set_variable name="destroy_z" value="z" />	 
	 <set_variable name="x" value="x+1" />
	 
     <destroy_near type="all" radius="100000" centerX="50000" centerY="0.0" centerZ="50000" />
	 
	 <incoming_comms_text from="DSTAN AND ONBOARD NAV">Current position (|x|, |z|)</incoming_comms_text>
	</event>

