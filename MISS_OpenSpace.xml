	<event name_arme="PositionTopZ">
	 <if_object_property name="Artemis" property="positionZ" comparator="LESS" value="10"/>
	 <set_object_property property="positionZ" name="Artemis" value="99900"/>
	 <set_variable name="IsDestroy" value="1" />	 
	 <set_variable name="destroy_x" value="x" />
	 <set_variable name="destroy_z" value="z" />	 
	 <set_variable name="z" value="z+1" />	 

     <destroy_near type="all" radius="100000" centerX="50000" centerY="0.0" centerZ="50000" />
	 
	 <incoming_comms_text from="DSTAN AND ONBOARD NAV">Current position (|x|, |z|)</incoming_comms_text>
	</event>

